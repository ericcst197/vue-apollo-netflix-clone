{
    "name": "vue-netflix-clone",
    "version": "0.0.0",
    "scripts": {
        "dev": "pnpm codegen && vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint .",
        "codegen": "cross-env DOTENV_CONFIG_PATH=.env graphql-codegen --config ./codegen.yml --require dotenv/config",
        "build-staging": "NODE_OPTIONS=--max-old-space-size=8192 vite build --mode staging",
        "build-production": "NODE_OPTIONS=--max-old-space-size=8192 vite build --mode production"
    },
    "dependencies": {
        "@apollo/client": "^3.8.8",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@graphql-codegen/typescript-vue-apollo": "^4.1.0",
        "@headlessui/vue": "^1.7.14",
        "@prismicio/helpers": "^2.3.9",
        "@prismicio/vue": "^4.0.2",
        "@vue/apollo-composable": "4.0.0-beta.12",
        "@vue/apollo-option": "4.0.0-beta.12",
        "@vueuse/core": "^10.7.0",
        "apollo-client": "^2.6.10",
        "eslint": "^8.42.0",
        "graphql-tag": "^2.12.6",
        "graphql-ws": "^5.16.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "pinia": "^2.1.7",
        "vite-plugin-graphql-codegen": "^3.3.5",
        "vue": "^3.4.3",
        "vue-router": "^4.2.2",
        "vueperslides": "^3.5.1",
        "youtube-player": "^5.6.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.39.5",
        "@graphql-codegen/cli": "^5.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@types/lodash": "^4.17.0",
        "@types/node": "^20.2.5",
        "@types/youtube-player": "^5.5.11",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.24",
        "tailwindcss": "^3.3.2",
        "tailwindcss-3d": "^1.0.5",
        "typescript": "^5.0.2",
        "unplugin-auto-import": "^0.17.5",
        "vite": "^4.3.9",
        "vue-tsc": "^1.4.2"
    }
}
